<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/linearLayout1" android:layout_width="fill_parent"
	android:orientation="vertical" android:layout_height="fill_parent">
	<LinearLayout android:id="@+id/linearLayout2"
		android:layout_height="wrap_content" android:layout_width="match_parent">
		<TextView android:text="スタート時の回転数　" android:id="@+id/kako"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		<EditText android:inputType="number" android:layout_height="50dp"
			android:text="0" android:id="@+id/kakok" android:layout_width="100dp"></EditText>
	</LinearLayout>
	<LinearLayout android:id="@+id/linearLayout3"
		android:layout_height="wrap_content" android:layout_width="match_parent">
		<TextView android:text="　  現在の回転数 　　" android:id="@+id/ima"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		<EditText android:inputType="number" android:layout_height="50dp"
			android:text="0" android:id="@+id/imak" android:layout_width="100dp"></EditText>
	</LinearLayout>
</LinearLayout>
